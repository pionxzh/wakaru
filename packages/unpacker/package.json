{
  "name": "@wakaru/unpacker",
  "type": "module",
  "version": "0.1.1",
  "description": "ðŸ”ªðŸ“¦ Unminify and beautify bundled code",
  "author": "Pionxzh",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "bin": "dist/cli.cjs",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run --globals",
    "test:update": "vitest run --update --globals",
    "test:watch": "vitest watch --globals",
    "test:type": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@types/jscodeshift": "^0.11.11",
    "@wakaru/ast-utils": "workspace:*",
    "@wakaru/shared": "workspace:*",
    "@wakaru/test-utils": "workspace:*",
    "ast-types": "^0.16.1",
    "jscodeshift": "^0.15.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
}
