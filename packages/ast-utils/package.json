    "test:watch": "vitest watch --globals",
    "test:type": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "devDependencies": {
    "@babel/helper-validator-identifier": "^7.22.20",
