{
  "name": "@wakaru/ast-utils",
  "type": "module",
  "version": "0.0.1",
  "description": "ðŸ”ªðŸ“¦ Unminify and beautify bundled code",
  "author": "Pionxzh",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./assert": "./src/assert.ts",
    "./case": "./src/case.ts",
    "./comments": "./src/comments.ts",
    "./exports": "./src/exports.ts",
    "./identifier": "./src/identifier.ts",
    "./imports": "./src/imports.ts",
    "./insert": "./src/insert.ts",
    "./matchers": "./src/matchers/index.ts",
    "./object": "./src/object.ts",
    "./parenthesized": "./src/parenthesized.ts",
    "./position": "./src/position.ts",
    "./program": "./src/program.ts",
    "./reference": "./src/reference.ts",
    "./scope": "./src/scope.ts",
    "./types": "./src/types.ts",
    "./wrapAstTransformation": "./src/wrapAstTransformation.ts"
