{
  "name": "@wakaru/ast-utils",
  "type": "module",
  "version": "0.0.1",
  "description": "ðŸ”ªðŸ“¦ Unminify and beautify bundled code",
  "author": "Pionxzh",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "files": [
    "dist",
    "package.json"
