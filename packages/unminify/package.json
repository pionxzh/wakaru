{
  "name": "@unminify-kit/unminify",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.3",
  "description": "ðŸ”ªðŸ“¦ Unminify and beautify bundled code",
  "author": "Pionxzh",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean",
    "test": "vitest run --globals",
    "test:update": "vitest run --update --globals",
    "test:watch": "vitest watch --globals",
    "test:type": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@babel/helper-validator-identifier": "^7.22.5",
    "@babel/preset-env": "^7.22.10",
    "@babel/types": "^7.22.10",
    "@unminify-kit/ast-utils": "workspace:*",
    "fs-extra": "^11.1.1",
    "globby": "^11.1.0",
    "jscodeshift": "^0.15.0",
    "lebab": "^3.2.1",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.8",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jscodeshift": "^0.11.6",
    "@types/prettier": "^2.7.3",
    "@types/yargs": "^17.0.24",
    "ast-types": "^0.16.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
}
